local function shieldTab()
    clearContent()
    local isOn = false
    local remote = ReplicatedStorage:FindFirstChild("BridgeNet2") and ReplicatedStorage.BridgeNet2:FindFirstChild("dataRemoteEvent")

    local btn = Instance.new("TextButton", contentFrame)
    btn.Size = UDim2.new(0, 140, 0, 40)
    btn.Position = UDim2.new(0.5, -70, 0.5, -20)
    btn.Text = "T·∫Øt"
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 16
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.BackgroundColor3 = Color3.fromRGB(255,60,60)

    btn.MouseButton1Click:Connect(function()
        isOn = not isOn
        btn.Text = isOn and "B·∫≠t" or "T·∫Øt"
        btn.BackgroundColor3 = isOn and Color3.fromRGB(60,255,60) or Color3.fromRGB(255,60,60)
        local args = {{isOn,"\024"}}
        if remote then pcall(function() remote:FireServer(unpack(args)) end) end
    end)
end

local function teleTab()
    clearContent()
    local locations = {
        ["C·ªïng 1"] = Vector3.new(18.867, 0.372, -114.862),
        ["C·ªïng 2"] = Vector3.new(18.373, 0.372, -140.728)
    }

    local function teleport(pos)
        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:FindFirstChild("HumanoidRootPart")
        if hrp then hrp.CFrame = CFrame.new(pos + Vector3.new(0,5,0)) end
    end

    local btn1 = Instance.new("TextButton", contentFrame)
    btn1.Size = UDim2.new(0, 160, 0, 35)
    btn1.Position = UDim2.new(0.5, -80, 0, 50)
    btn1.Text = "TP C·ªïng 1"
    btn1.BackgroundColor3 = Color3.fromRGB(70,130,255)
    btn1.TextColor3 = Color3.new(1,1,1)

    local btn2 = Instance.new("TextButton", contentFrame)
    btn2.Size = UDim2.new(0, 160, 0, 35)
    btn2.Position = UDim2.new(0.5, -80, 0, 95)
    btn2.Text = "TP C·ªïng 2"
    btn2.BackgroundColor3 = Color3.fromRGB(70,255,130)
    btn2.TextColor3 = Color3.new(1,1,1)

    btn1.MouseButton1Click:Connect(function() teleport(locations["C·ªïng 1"]) end)
    btn2.MouseButton1Click:Connect(function() teleport(locations["C·ªïng 2"]) end)
end

local function tp1Tab()
    clearContent()
    local lock = false
    local target = Vector3.new(40.00, 3.63, -130.14)
    local btn = Instance.new("TextButton", contentFrame)
    btn.Size = UDim2.new(0, 160, 0, 35)
    btn.Position = UDim2.new(0.5, -80, 0.5, -20)
    btn.Text = "B·∫≠t kh√≥a v·ªã tr√≠"
    btn.Font = Enum.Font.GothamBold
    btn.TextColor3 = Color3.new(1,1,1)
    btn.BackgroundColor3 = Color3.fromRGB(0,170,0)

    RunService.Heartbeat:Connect(function()
        if lock then
            local char = player.Character
            if char and char:FindFirstChild("HumanoidRootPart") then
                local hrp = char.HumanoidRootPart
                local hum = char:FindFirstChildOfClass("Humanoid")
                hrp.Velocity, hrp.RotVelocity = Vector3.zero, Vector3.zero
                hrp.CFrame = CFrame.new(target)
                if hum then hum.WalkSpeed, hum.JumpPower = 0, 0 end
            end
        end
    end)

    btn.MouseButton1Click:Connect(function()
        lock = not lock
        if lock then
            btn.Text = "T·∫Øt kh√≥a v·ªã tr√≠"
            btn.BackgroundColor3 = Color3.fromRGB(200,50,50)
            StarterGui:SetCore("SendNotification",{Title="‚öì ƒê√£ kh√≥a v·ªã tr√≠!",Text=string.format("X:%.2f Y:%.2f Z:%.2f",target.X,target.Y,target.Z),Duration=3})
        else
            btn.Text = "B·∫≠t kh√≥a v·ªã tr√≠"
            btn.BackgroundColor3 = Color3.fromRGB(0,170,0)
            local hum = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
            if hum then hum.WalkSpeed, hum.JumpPower = 16, 50 end
            StarterGui:SetCore("SendNotification",{Title="‚úÖ ƒê√£ m·ªü kh√≥a di chuy·ªÉn",Text="B·∫°n c√≥ th·ªÉ di chuy·ªÉn t·ª± do.",Duration=3})
        end
    end)
end

local function tp2Tab()
    clearContent()
    local lock = false
    local target = Vector3.new(42.29, 3.63, -132.49)
    local look = nil

    local btn = Instance.new("TextButton", contentFrame)
    btn.Size = UDim2.new(0, 180, 0, 35)
    btn.Position = UDim2.new(0.5, -90, 0.5, -20)
    btn.Text = "B·∫≠t kh√≥a v·ªã tr√≠"
    btn.Font = Enum.Font.GothamBold
    btn.TextColor3 = Color3.new(1,1,1)
    btn.BackgroundColor3 = Color3.fromRGB(0,170,0)

    RunService.Heartbeat:Connect(function()
        if lock then
            local char = player.Character
            if char and char:FindFirstChild("HumanoidRootPart") then
                local hrp = char.HumanoidRootPart
                local hum = char:FindFirstChildOfClass("Humanoid")
                if look then hrp.CFrame = CFrame.new(target, target + look) end
                hrp.Velocity, hrp.RotVelocity = Vector3.zero, Vector3.zero
                if hum then hum.WalkSpeed, hum.JumpPower = 0, 0 end
            end
        end
    end)

    btn.MouseButton1Click:Connect(function()
        lock = not lock
        local char = player.Character
        if lock then
            if char and char:FindFirstChild("HumanoidRootPart") then
                look = char.HumanoidRootPart.CFrame.LookVector
            end
            btn.Text = "T·∫Øt kh√≥a v·ªã tr√≠"
            btn.BackgroundColor3 = Color3.fromRGB(200,50,50)
            StarterGui:SetCore("SendNotification",{Title="‚öì ƒê√£ kh√≥a v·ªã tr√≠!",Text="Gi·ªØ h∆∞·ªõng nh√¨n hi·ªán t·∫°i",Duration=3})
        else
            if char and char:FindFirstChildOfClass("Humanoid") then
                local hum = char:FindFirstChildOfClass("Humanoid")
                hum.WalkSpeed, hum.JumpPower = 16, 50
            end
            look = nil
            btn.Text = "B·∫≠t kh√≥a v·ªã tr√≠"
            btn.BackgroundColor3 = Color3.fromRGB(0,170,0)
            StarterGui:SetCore("SendNotification",{Title="‚úÖ ƒê√£ m·ªü kh√≥a di chuy·ªÉn",Text="B·∫°n c√≥ th·ªÉ di chuy·ªÉn v√† xoay h∆∞·ªõng.",Duration=3})
        end
    end)
end

--// G√ÅN H√ÄM V√ÄO TAB
local tabFunctions = {
    ["Fast M1"] = fastM1Tab,
    ["Spam M1"] = spamM1Tab,
    ["Spam M2"] = spamM2Tab,
    ["Shield"] = shieldTab,
    ["Tele"] = teleTab,
    ["TP1"] = tp1Tab,
    ["TP2"] = tp2Tab
}

for name, btn in pairs(tabs) do
    btn.MouseButton1Click:Connect(function()
        for _, b in pairs(tabs) do b.BackgroundColor3 = Color3.fromRGB(60,60,60) end
        btn.BackgroundColor3 = Color3.fromRGB(100,100,100)
        if tabFunctions[name] then pcall(tabFunctions[name]) end
    end)
end

-- Auto open first tab
pcall(function() tabs["Fast M1"].BackgroundColor3 = Color3.fromRGB(100,100,100); fastM1Tab() end)
print("[‚úÖ Game Hacker Multi Tool] ƒê√£ t·∫£i xong GUI!")
